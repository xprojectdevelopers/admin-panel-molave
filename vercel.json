{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "mobile.html", "use": "@vercel/static" },
    { "src": "api/config.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/config", "dest": "/api/config.js" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/mobile", "dest": "/mobile.html" }
  ]
}
